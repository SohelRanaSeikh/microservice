pipeline {
    agent any
    stages {
        stage('Welcome Step') {
            steps {
                echo 'Welcome to LambdaTest'
            }
        }
        stage('compile') {
              steps {
                      bat 'mvn compile'
                     }
                   }
        stage('build') {
                    steps {
                    bat 'mvn install'
                    }
                }
         stage('test') {
                  steps {
                      bat 'mvn test'
                         }
                      }
           stage('Quality') {
                             steps {
                                 //bat 'mvn test'
                                 echo 'from quality stage'
                                 bat 'mvn sonar:sonar'
                                    }
                                 }
    }
}